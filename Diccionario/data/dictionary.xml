<?xml version="1.0" encoding="UTF-8"?>
<dictionary xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="dictionary_schema.xsd">
  <entry id="en_1">
    <word>book</word>
    <phonetic>bʊk</phonetic>
    <language>en</language>
    <translations>
      <translation>
        <language>es</language>
        <word>libro</word>
        <phonetic>ˈli.βɾo</phonetic>
      </translation>
      <translation>
        <language>fr</language>
        <word>livre</word>
        <phonetic>livʁ</phonetic>
      </translation>
      <translation>
        <language>de</language>
        <word>Buch</word>
        <phonetic>buːx</phonetic>
      </translation>
    </translations>
    <definitions>
      <definition>
        <text>A written or printed work consisting of pages glued or sewn together along one side and bound in covers.</text>
        <partOfSpeech>noun</partOfSpeech>
        <examples>
          <example>I'm reading a good book at the moment.</example>
          <example>She wrote a book on modern art.</example>
        </examples>
      </definition>
      <definition>
        <text>To reserve (something, especially a hotel room, seat, etc.) for future use.</text>
        <partOfSpeech>verb</partOfSpeech>
        <examples>
          <example>I've booked a table for two at 8 o'clock.</example>
          <example>You need to book in advance to get tickets.</example>
        </examples>
      </definition>
    </definitions>
    <synonyms>
      <synonym>volume</synonym>
      <synonym>publication</synonym>
      <synonym>tome</synonym>
    </synonyms>
    <usage>
      <register>standard</register>
      <context>formal, informal</context>
      <region>global</region>
    </usage>
    <etymology>From Old English bōc, from Proto-Germanic *bōks.</etymology>
  </entry>
  
  <entry id="es_1">
    <word>libro</word>
    <phonetic>ˈli.βɾo</phonetic>
    <language>es</language>
    <translations>
      <translation>
        <language>en</language>
        <word>book</word>
        <phonetic>bʊk</phonetic>
      </translation>
      <translation>
        <language>fr</language>
        <word>livre</word>
        <phonetic>livʁ</phonetic>
      </translation>
      <translation>
        <language>de</language>
        <word>Buch</word>
        <phonetic>buːx</phonetic>
      </translation>
    </translations>
    <definitions>
      <definition>
        <text>Conjunto de hojas de papel, pergamino, vitela, etc., manuscritas o impresas, unidas por uno de sus lados y normalmente encuadernadas.</text>
        <partOfSpeech>nombre</partOfSpeech>
        <examples>
          <example>Estoy leyendo un buen libro actualmente.</example>
          <example>Ella escribió un libro sobre arte moderno.</example>
        </examples>
      </definition>
    </definitions>
    <synonyms>
      <synonym>volumen</synonym>
      <synonym>obra</synonym>
      <synonym>texto</synonym>
    </synonyms>
    <usage>
      <register>estándar</register>
      <context>formal, informal</context>
      <region>global</region>
    </usage>
    <etymology>Del latín liber, libri.</etymology>
  </entry>
</dictionary>